{
  "name": "Create Jira project",
  "description": "Create a Jira project",
    "documentation": [
    "projects/atlassian/jira/template/README.md"
  ],
  "appType": "executable",
  "inputs": [
    {
      "name": "Project name",
      "description": "Jira project name",
      "type": "text",
      "target": "executor",
      "field": "PROJECT_NAME"
    },
    {
    "name": "Board type",
    "description": "Board type",
    "type": "list",
    "target": "executor",
    "field": "BOARD_TYPE",
    "options": [
      {
        "text": "Create Kanban board",
        "value": "kanban"
      },
      {
        "text": "Create Scrum board",
        "value": "scrum"
      }
    ]
  }],
  "executable": "template/create_jira_project.py",
  "postActions": {
    "all": [],
    "GitHub": []
  }
}
